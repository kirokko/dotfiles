# Backward and Forward buttons
"xvkbd -text "\[Alt_L]\[Left]""
  m:0x10 + b:8
"xvkbd -text "\[Alt_L]\[Right]""
  m:0x10 + b:9

# "Cruise Control" disabled:
#"xvkbd -text "\[Page_Up]""
