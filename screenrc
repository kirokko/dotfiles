defscrollback 15000

shell -$SHELL

startup_message off
msgwait 1
vbell off

term screen-256color

caption always
#caption string "%w"
caption string "%{-b ..}%-w%{+b ..}%n%f*%t%{-}%+w"

escape ``

bind C screen -t 'mc' 4 env TERMCAP= mc -x

bind c screen 1
bind ^c screen 1
bind 0 select 10
screen 1

# EOL
